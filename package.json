{
  "name": "arm-template-builder",
  "version": "0.0.0",
  "description": "A Node.js Library to build Azure ARM Templates without the bulk.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "engines": {
    "node": ">= 12.0.0"
  },
  "license": "MIT",
  "keywords": [
    "azure",
    "arm",
    "azure-arm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyerleinf/node-arm-template-builder.git"
  },
  "bugs": {
    "url": "https://github.com/beyerleinf/node-arm-template-builder/issues"
  },
  "homepage": "https://github.com/beyerleinf/node-arm-template-builder#readme",
  "scripts": {
    "test": "nyc mocha",
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run lint",
    "commitlint": "commitlint --config commitlint.config.js --to",
    "typedoc": "typedoc"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.11.2",
    "@types/sinon": "^10.0.5",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-security-node": "^1.0.14",
    "gts": "^3.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.7",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
